{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "es6": true
    },
    "extends": ["google"],
    "rules": {
        "no-trailing-spaces" : "off",
        "one-var" : "off",
        "one-var-declaration-per-line" : "off",
        "require-jsdoc" : "off",
        "quotes" : "off",
        "comma-dangle" : ["error", "never"]
    }
}